{
  "name": "@postman/wormhole",
  "version": "1.0.0-alpha.1",
  "description": "A custom network layer for Postman.",
  "main": "index.js",
  "directories": {
    "test": "package_tests"
  },
  "scripts": {
    "build": "node-gyp rebuild",
    "clean": "node-gyp clean",
    "test": "mocha package_tests",
    "lint": "eslint index.js package_tests/",
    "format": "eslint --fix index.js package_tests/",
    "cmake:build": "cmake -S . -B cmake_build && cmake --build cmake_build && cmake --install cmake_build --prefix relocatable_libwormhole && install_name_tool -id @rpath/lib/libwormhole.dylib relocatable_libwormhole/lib/libwormhole.dylib",
    "cmake:format": "cmake --build cmake_build --target clang_format",
    "cmake:test": "cmake_build/test/request_unit_test",
    "cmake:all": "npm run cmake:build && npm run cmake:format && npm run cmake:test",
    "cmake:clean": "rm -rf cmake_build",
    "shell:lint": "cmake --build cmake_build --target shellcheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/postmanlabs/wormhole.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/postmanlabs/wormhole/issues"
  },
  "homepage": "https://github.com/postmanlabs/wormhole#readme",
  "devDependencies": {
    "eslint": "^8.36.0",
    "mocha": "^10.2.0"
  },
  "dependencies": {
    "node-addon-api": "^6.0.0"
  }
}
